{
  "agent": {
    "name": "AnalyzerAgent",
    "type": "AIAgent",
    "description": "Analyzes input data and extracts insights",
    "capabilities": [
      "data_analysis",
      "pattern_recognition",
      "summarization"
    ],
    "configuration": {
      "model": "gpt-4",
      "temperature": 0.7,
      "max_tokens": 2000
    },
    "tools": [
      "data_reader",
      "statistics_calculator"
    ],
    "guardrails": {
      "max_retries": 3,
      "timeout_seconds": 30,
      "rate_limit": {
        "requests_per_minute": 60
      }
    }
  }
}
