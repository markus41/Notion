{
  "name": "notion-webhook-agent-activity",
  "version": "1.0.0",
  "description": "Real-time webhook listener for Notion agent activity logging with queue-based fallback resilience",
  "main": "dist/NotionWebhook/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "prestart": "npm run build",
    "start": "func start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "deploy": "npm run build && func azure functionapp publish notion-webhook-brookside"
  },
  "dependencies": {
    "@azure/functions": "^4.5.0",
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.8.0",
    "@notionhq/client": "^2.2.14"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "azure-functions-core-tools": "^4.0.5455",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "azure-functions",
    "notion-webhook",
    "agent-activity-logging",
    "brookside-bi",
    "innovation-nexus"
  ],
  "author": "Brookside BI Innovation Nexus",
  "license": "MIT"
}
